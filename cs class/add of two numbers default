#include <stdio.h>

int main() {
    int a, b, c;
    printf("Enter the value of a: ");
    scanf("%d", &a);
    printf("Enter the value of b: ");
    scanf("%d", &b);
    if (a < b){
    
    c = a + b; 
    printf("Sum of two numbers = %d\n", c); 
    }
    
    else {
    
    c = a - b;
    printf("difference of two numbers = %d\n", c); 
    }
    
    return 0;
}
