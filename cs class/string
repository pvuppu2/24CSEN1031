#include <stdio.h>
#include <string.h>
int main() {

char name[] = "pranav sai";
printf("%d", strlen(name));

    return 0;
}



// output = 10







#include <stdio.h>
#include <string.h>

int main() {

char firstname[] = "Pranav";
char lastname[] = "Vuppu";

printf("%s", strcat(firstname, lastname));

    return 0;
}
// output = PranavVuppu


